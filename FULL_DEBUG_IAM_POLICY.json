{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "SNSDebug",
      "Effect": "Allow",
      "Action": [
        "sns:ListSubscriptionsByTopic",
        "sns:GetSubscriptionAttributes",
        "sns:GetTopicAttributes"
      ],
      "Resource": "arn:aws:sns:us-west-2:060163370478:*"
    },
    {
      "Sid": "EventBridgeDebug",
      "Effect": "Allow",
      "Action": [
        "events:ListTargetsByRule",
        "events:DescribeRule"
      ],
      "Resource": "arn:aws:events:us-west-2:060163370478:rule/*"
    },
    {
      "Sid": "MediaConvertDebug",
      "Effect": "Allow",
      "Action": [
        "mediaconvert:GetJob",
        "mediaconvert:ListJobs"
      ],
      "Resource": "*"
    },
    {
      "Sid": "S3Read",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::church-app-uploads-stevensills2",
        "arn:aws:s3:::church-app-uploads-stevensills2/*"
      ]
    },
    {
      "Sid": "LogsRead",
      "Effect": "Allow",
      "Action": [
        "logs:DescribeLogStreams",
        "logs:GetLogEvents"
      ],
      "Resource": "arn:aws:logs:us-west-2:060163370478:*"
    }
  ]
}
